{
  "presets": [
    "module:metro-react-native-babel-preset",
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true,
      }
    ],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./"
        ],
        "alias": {
          "fego-rn": "./components",
          "common": "./components/common"
        }
      },
    ]
  ]
}
